<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<GridPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.app.menu.result.ResultComponentController">
  <columnConstraints>
      <ColumnConstraints hgrow="NEVER" maxWidth="10.0" minWidth="10.0" prefWidth="10.0" />
    <ColumnConstraints hgrow="ALWAYS" maxWidth="250.0" minWidth="150.0" prefWidth="220.0" />
      <ColumnConstraints hgrow="NEVER" minWidth="-Infinity" prefWidth="10.0" />
    <ColumnConstraints hgrow="ALWAYS" minWidth="-Infinity" prefWidth="600.0" />
      <ColumnConstraints hgrow="NEVER" maxWidth="10.0" minWidth="10.0" prefWidth="10.0" />
  </columnConstraints>
  <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="10.0" vgrow="NEVER" />
      <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0" vgrow="NEVER" />
    <RowConstraints maxHeight="400.0" minHeight="-Infinity" prefHeight="220.0" vgrow="ALWAYS" />
      <RowConstraints maxHeight="10.0" minHeight="10.0" prefHeight="10.0" vgrow="NEVER" />
      <RowConstraints maxHeight="500.0" minHeight="10.0" prefHeight="220.0" vgrow="ALWAYS" />
      <RowConstraints maxHeight="10.0" minHeight="10.0" prefHeight="10.0" vgrow="NEVER" />
  </rowConstraints>
   <children>
      <VBox GridPane.columnIndex="3" GridPane.rowIndex="2">
         <children>
            <AnchorPane maxHeight="370.0" VBox.vgrow="ALWAYS">
               <children>
                  <fx:include fx:id="executionDetailsComponent" minHeight="100.0" minWidth="100.0" source="details/ExcecutionDetailsComponent.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
               </children>
            </AnchorPane>
         </children>
      </VBox>
      <AnchorPane GridPane.columnIndex="3" GridPane.rowIndex="4">
         <children>
            <fx:include fx:id="resultTabComponent" source="tab/ResultTabComponent.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
         </children>
      </AnchorPane>
      <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="2" GridPane.rowSpan="3">
         <children>
            <fx:include fx:id="executionQueueComponent" source="queue/ExecutionQueueComponent.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
         </children>
      </AnchorPane>
   </children>
</GridPane>
