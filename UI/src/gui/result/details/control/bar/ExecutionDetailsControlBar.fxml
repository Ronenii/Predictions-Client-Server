<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" styleClass="Control-Bar" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.result.details.control.bar.ExecutionDetailsControlBarController">
  <columnConstraints>
    <ColumnConstraints hgrow="NEVER" minWidth="10.0" />
    <ColumnConstraints hgrow="NEVER" minWidth="10.0" />
      <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="143.0" />
      <ColumnConstraints hgrow="NEVER" maxWidth="99.0" minWidth="10.0" prefWidth="81.0" />
      <ColumnConstraints hgrow="NEVER" minWidth="10.0" prefWidth="10.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" vgrow="NEVER" />
  </rowConstraints>
   <children>
      <Button id="btn-ExecDetails-Start" fx:id="playBTN" mnemonicParsing="false" onAction="#playButtonActionListener" text="▶" GridPane.columnIndex="3" GridPane.halignment="RIGHT" GridPane.hgrow="NEVER" GridPane.vgrow="NEVER" />
      <Button id="btn-ExecDetails-Pause" fx:id="pauseBTN" mnemonicParsing="false" onAction="#pauseButtonActionListener" text="⏸" GridPane.columnIndex="3" GridPane.halignment="LEFT" GridPane.hgrow="NEVER" GridPane.vgrow="NEVER" />
      <Button id="btn-ExecDetails-Stop" fx:id="stopBTN" mnemonicParsing="false" onAction="#stopButtonActionListener" text="⏹" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.hgrow="NEVER" />
   </children>
</GridPane>
