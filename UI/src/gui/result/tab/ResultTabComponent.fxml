<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.result.tab.ResultTabComponentController">
   <children>
      <Label fx:id="executionResultLabel" text="Excecution Result" VBox.vgrow="NEVER">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <TabPane fx:id="executionResultTP" disable="true" maxHeight="500.0" opacity="0.2" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
         <tabs>
            <Tab text="Chart">
               <content>
                  <ScrollPane>
                     <content>
                        <AnchorPane>
                           <children>
                              <fx:include fx:id="chartComponent" minHeight="-Infinity" prefHeight="200.0" source="chart/ChartComponent.fxml" />
                           </children>
                        </AnchorPane>
                     </content>
                  </ScrollPane>
               </content></Tab>
            <Tab text="Statistics">
               <content>
                  <ScrollPane>
                     <content>
                        <AnchorPane>
                           <children>
                              <fx:include fx:id="statisticsComponent" source="statistics/StatisticsComponent.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                           </children>
                        </AnchorPane>
                     </content>
                  </ScrollPane>
               </content></Tab>
         </tabs>
      </TabPane>
   </children>
</VBox>
